{
  "name": "dilite",
  "version": "3.0.0",
  "description": "A light weight DI container",
  "homepage": "https://www.github.com/chetanism/dilite",
  "repository": "chetanism/dilite",
  "author": {
    "name": "Chetan Verma",
    "email": "verma.chetan@gmail.com"
  },
  "license": "ISC",
  "keywords": [
    "DI",
    "IoC",
    "dependency injection",
    "container"
  ],
  "main": "lib/index.js",
  "babel": {
    "ignore": [
      "**/*.test.js"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "8"
          }
        }
      ]
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "jest": {
    "testURL": "http://localhost"
  },
  "dependencies": {
    "public-class": "0.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^23.4.2",
    "prettier": "^1.14.0"
  },
  "scripts": {
    "test": "jest",
    "prettier": "prettier --write \"src/**/*.js\"",
    "clean": "rm -rf lib",
    "compile": "babel src -d lib",
    "build": "npm run prettier && npm run test && npm run clean && npm run compile"
  }
}
